{
  "name": "jogging-tracker",
  "version": "0.0.1",
  "description": "React project with TypeScript and WebPack",
  "private": true,
  "main": "dist/main.bundle.js",
  "scripts": {
    "start": "react-ts -- serve 3000",
    "build": "react-ts -- build && npm run precache",
    "build:dev": "react-ts -- build development",
    "lint": "tslint src/**/*.tsx",
    "precache": "sw-precache --config=sw-precache-config.js --verbose",
    "test": "nyc mocha src/**/*.spec.ts*",
    "tdd": "nodemon -e ts,tsx --watch src --exec 'npm t'",
    "watch": "npm run compile -- -w"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 95,
    "statements": 95,
    "functions": 95,
    "branches": 95,
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/node_modules/**",
      "**/.git/**",
      "**/dist/**"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "ts-node/register",
      "./src/test/ignore.js"
    ],
    "cache": true
  },
  "devDependencies": {
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.12",
    "@types/react": "^15.0.21",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "chalk": "^1.1.3",
    "concurrently": "^3.4.0",
    "enzyme": "^2.9.1",
    "istanbul": "^0.4.5",
    "jsdom": "11.1.0",
    "jsdom-global": "3.0.2",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "nyc": "^11.0.3",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "react-ts": "^2.0.2",
    "require-hacker": "^3.0.0",
    "sw-precache": "^5.2.0",
    "ts-node": "^3.3.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "font-awesome": "^4.7.0",
    "material-ui": "^1.0.0-alpha.22",
    "moment": "^2.18.1",
    "numeral": "^2.0.6",
    "react": "^15.5.3",
    "react-router-dom": "^4.1.2",
    "reflect-metadata": "^0.1.10",
    "statex": "^1.0.1",
    "tachyons": "^4.7.4"
  }
}